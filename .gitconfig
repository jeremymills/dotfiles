[user]
  name = Jeremy Mills
  email = jeremy@rithm.me
[core]
  editor = vim
[github]
  user = jeremymills
[color]
  ui = auto
[color "status"]
  added = 23
  changed = 255
  untracked = 1
[alias]
  a  = add
  c  = commit
  g  = grep
  f  = fetch
  ps = push
  pl = pull
  br = branch
  co = checkout
  st = status
  lg = log --pretty=oneline -n 50 --graph --abbrev-commit
  cl = "!git branch --merged | grep -v '\\*' | xargs -n 1 git branch -d"
  cr = remote update --prune
  unstage = reset HEAD --
[push]
  default = simple
[rerere]
  enabled = 1
[credential]
  helper = osxkeychain
